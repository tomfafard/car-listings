<!DOCTYPE html>
<html>
<head>
  <title>CarListings</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% flash.each do |key, val|%>
    <div class="flash flash-<%=key%>">
      <%= val %>
    </div>
  <% end %>

<%= yield %>

</body>
</html>
